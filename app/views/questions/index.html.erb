<h3>/All QUESTIONS/</h3>

<p>
<%= link_to '| CREATE NEW QUESTION |', new_test_question_path %>
</p>

<table>
  <tr>
    <th>Body:</th>
    <th colspan="3">Actions</th>
  <tr>

  <% @questions.each do |question| %>
    <tr>
      <td><%= question.body %></td>
      <td><%= question.test.title %></td>
      <td><%= link_to '| SHOW |', test_question_path(question.test, question) %></td>
      <td><%= link_to '| EDIT |', edit_test_question_path(question, question.test) %></td>
      <td><%= link_to '| TEST |', [@test] %></td>
    </tr>
  <% end %>
</table>
