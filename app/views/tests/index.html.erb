<p class="basis">
<h2>/All TESTS/</h2>

<p>
<%= link_to '/ Create New Test / ', new_test_path %>
</p>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Count Questions</th>
    <th colspan="5">Actions</th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= test.questions.count %></td>
      <td><%= link_to '/ Create User /', signup_path %></td>
      <td><%= link_to '/ Show /', test_path(test) %></td>
      <td><%= link_to '/ Edit /', edit_test_path(test) %></td>
      <td><%= link_to '/ Delete /',
                        test_path(test),
                        method: :delete,
                        data: { confirm: 'Are You Sure?' } %></td>
      <td><%= button_to ' Start Test -> ', start_test_path(test) %></td>
    </tr>
  <% end %>
</table>
</p>